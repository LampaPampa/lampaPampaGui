Section:NewButton("create a car", "ы", function()
--[[
    Скрипт для создания и управления управляемой машиной в Roblox.
    Этот скрипт предназначен для инжекции и должен быть запущен
    на клиенте игрока.

    Предполагается, что модель машины уже существует в пространстве (Workspace)
    и имеет следующую структуру:
    - CarModel (Модель)
        - Body (Part) - Основная часть, к которой будут прикреплены физические движки.
        - WheelFrontLeft (Wheel)
        - WheelFrontRight (Wheel)
        - WheelBackLeft (Wheel)
        - WheelBackRight (Wheel)
    Так же предполагает, что на 'Body' уже настроен BodyVelocity или AlignPosition
    для перемещения.

    КАК ИСПОЛЬЗОВАТЬ:
    1. Создайте модель машины в Roblox Studio или найдите готовую.
    2. Убедитесь, что модель имеет указанную структуру.
    3. Настройте BodyVelocity на основной части (Body) вашей машины.
       - MaxForce, P, D будут определять скорость и отзывчивость.
    4. "Инжектируйте" этот скрипт в клиент игры.
]]

-- Параметры машины
local CAR_MODEL_NAME = "MyAwesomeCar" -- Укажите имя вашей модели машины
local STEERING_FORCE = 5000 -- Сила поворота
local ENGINE_FORCE = 20000 -- Сила ускорения
local BRAKE_FORCE = 30000  -- Сила торможения
local MAX_SPEED = 100      -- Ограничение максимальной скорости (для BodyVelocity)

-- Настройки управления
local FORWARD_KEY = Enum.KeyCode.W
local BACKWARD_KEY = Enum.KeyCode.S
local LEFT_KEY = Enum.KeyCode.A
local RIGHT_KEY = Enum.KeyCode.D
local BRAKE_KEY = Enum.KeyCode.Space

-- Глобальные переменные
local carModel = nil
local player = game.Players.LocalPlayer
local userInputService = game:GetService("UserInputService")
local runService = game:GetService("RunService")

local currentThrottle = 0
local currentSteering = 0
local isBraking = false

-- Поиск модели машины
local function findCarModel()
    for _, model in pairs(workspace:GetChildren()) do
        if model:IsA("Model") and model.Name == CAR_MODEL_NAME then
            -- Проверяем, есть ли необходимые части
            if model:FindFirstChild("Body") and
               model:FindFirstChild("WheelFrontLeft") and
               model:FindFirstChild("WheelFrontRight") and
               model:FindFirstChild("WheelBackLeft") and
               model:FindFirstChild("WheelBackRight") then
                return model
            end
        end
    end
    return nil
end

-- Функция для обработки ввода игрока
local function handleInput(input, gameProcessedEvent)
    if gameProcessedEvent then return end

    local key = input.KeyCode

    if input.UserInputType == Enum.UserInputType.Keyboard then
        if key == FORWARD_KEY then
            currentThrottle = 1
        elseif key == BACKWARD_KEY then
            currentThrottle = -1
        elseif key == LEFT_KEY then
            currentSteering = -1
        elseif key == RIGHT_KEY then
            currentSteering = 1
        elseif key == BRAKE_KEY then
            isBraking = true
        end
    end
end

-- Функция для сброса управления
local function resetControls()
    currentThrottle = 0
    currentSteering = 0
    isBraking = false
end

-- Обработка отпускания клавиш
local function handleInputEnded(input, gameProcessedEvent)
    if gameProcessedEvent then return end

    local key = input.KeyCode

    if input.UserInputType == Enum.UserInputType.Keyboard then
        if key == FORWARD_KEY and currentThrottle == 1 then
            currentThrottle = 0
        elseif key == BACKWARD_KEY and currentThrottle == -1 then
            currentThrottle = 0
        elseif key == LEFT_KEY and currentSteering == -1 then
            currentSteering = 0
        elseif key == RIGHT_KEY and currentSteering == 1 then
            currentSteering = 0
        elseif key == BRAKE_KEY then
            isBraking = false
        end
    end
end

-- Основная функция управления машиной
local function updateCar()
    if not carModel then return end

    local body = carModel:FindFirstChild("Body")
    if not body then return end

    local bodyVelocity = body:FindFirstChildOfClass("BodyVelocity")
    if not bodyVelocity then
        -- Если BodyVelocity не найден, можно добавить его или вывести сообщение
        warn("BodyVelocity не найден на части 'Body' машины: " .. carModel.Name)
        return
    end

    local direction = body.CFrame.LookVector * currentThrottle
    local moveForce = direction * ENGINE_FORCE

    -- Управление ускорением и торможением
    if isBraking then
        bodyVelocity.Velocity = Vector3.new(0, 0, 0) -- Полная остановка
        bodyVelocity.MaxForce = Vector3.new(math.huge, 0, math.huge) * BRAKE_FORCE -- Применяем сильное торможение
    else
        bodyVelocity.MaxForce = Vector3.new(math.huge, 0, math.huge) * ENGINE_FORCE -- Применяем силу движения
        bodyVelocity.Velocity = direction * MAX_SPEED -- Ограничиваем скорость
    end

    -- Управление поворотом (примерно, может потребоваться настройка)
    -- Для поворота нужно изменять CFrame или применять силы к колесам
    -- Этот пример упрощен и предполагает, что BodyVelocity может быть
    -- направлена, но реальный поворот требует вращения колес.

    -- Пример для поворота:
    local steeringAxis = carModel.CFrame.UpVector -- Ось поворота (предполагаем, что Y - это вверх)

    -- Поворот передних колес (для более реалистичного управления)
    -- Это сложная часть, требующая обращения к механизмам вращения колес.
    -- В этом простом примере мы можем попытаться немного повернуть весь корпус.

    if currentSteering ~= 0 then
        -- Поворот кузова (очень упрощенно)
        local turnAmount = STEERING_FORCE * currentSteering
        carModel:SetPivot(carModel:GetPivot() * CFrame.Angles(0, math.rad(turnAmount * runService.Heartbeat:Wait()), 0))
    end
end

-- Основной цикл скрипта
local function gameLoop()
    if not carModel then
        carModel = findCarModel()
        if carModel then
            print("Машина '" .. CAR_MODEL_NAME .. "' найдена!")
            -- Подписываемся на ввод игрока
            userInputService.InputBegan:Connect(handleInput)
            userInputService.InputEnded:Connect(handleInputEnded)
        end
    end

    if carModel then
        updateCar()
    end
end

-- Подписка на событие Heartbeat для постоянного обновления
runService.Heartbeat:Connect(gameLoop)

print("Скрипт управляемой машины запущен. Ищет модель '" .. CAR_MODEL_NAME .. "'...")
end)
