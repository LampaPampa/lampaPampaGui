Status.Name = "Status"
Status.Parent = BG
Status.BackgroundColor3 = Color3.new(1, 1, 1)
Status.BackgroundTransparency = 1
Status.Position = UDim2.new(0.580952346, 0, 0.708661377, 0)
Status.Size = UDim2.new(0, 56, 0, 20)
Status.Font = Enum.Font.Highway
Status.FontSize = Enum.FontSize.Size14
Status.Text = "off"
Status.TextColor3 = Color3.new(0.666667, 0, 0)
Status.TextScaled = true
Status.TextSize = 14
Status.TextStrokeColor3 = Color3.new(0.180392, 0, 0.431373)
Status.TextWrapped = true
Status.TextXAlignment = Enum.TextXAlignment.Left


Toggle.MouseButton1Click:connect(function()
    if Status.Text == "off" then
        Clipon = true
        Status.Text = "on"
        Status.TextColor3 = Color3.new(0,185,0)
        Stepped = game:GetService("RunService").Stepped:Connect(function()
            if not Clipon == false then
                for a, b in pairs(Workspace:GetChildren()) do
                if b.Name == Plr.Name then
                for i, v in pairs(Workspace[Plr.Name]:GetChildren()) do
                if v:IsA("BasePart") then
                v.CanCollide = false
                end end end end
            else
                Stepped:Disconnect()
            end
        end)
    elseif Status.Text == "on" then
        Clipon = false
        Status.Text = "off"
        Status.TextColor3 = Color3.new(170,0,0)
    end
end)
    else
        print("Toggle Off")
    end
end)

Section:NewButton("create a car", "—ã", function()
--[[
    –°–∫—Ä–∏–ø—Ç –¥–ª—è —Å–æ–∑–¥–∞–Ω–∏—è –∏ —É–ø—Ä–∞–≤–ª–µ–Ω–∏—è —É–ø—Ä–∞–≤–ª—è–µ–º–æ–π –º–∞—à–∏–Ω–æ–π –≤ Roblox.
    –≠—Ç–æ—Ç —Å–∫—Ä–∏–ø—Ç –ø—Ä–µ–¥–Ω–∞–∑–Ω–∞—á–µ–Ω –¥–ª—è –∏–Ω–∂–µ–∫—Ü–∏–∏ –∏ –¥–æ–ª–∂–µ–Ω –±—ã—Ç—å –∑–∞–ø—É—â–µ–Ω
    –Ω–∞ –∫–ª–∏–µ–Ω—Ç–µ –∏–≥—Ä–æ–∫–∞.

    –ü—Ä–µ–¥–ø–æ–ª–∞–≥–∞–µ—Ç—Å—è, —á—Ç–æ –º–æ–¥–µ–ª—å –º–∞—à–∏–Ω—ã —É–∂–µ —Å—É—â–µ—Å—Ç–≤—É–µ—Ç –≤ –ø—Ä–æ—Å—Ç—Ä–∞–Ω—Å—Ç–≤–µ (Workspace)
    –∏ –∏–º–µ–µ—Ç —Å–ª–µ–¥—É—é—â—É—é —Å—Ç—Ä—É–∫—Ç—É—Ä—É:
    - CarModel (–ú–æ–¥–µ–ª—å)
        - Body (Part) - –û—Å–Ω–æ–≤–Ω–∞—è —á–∞—Å—Ç—å, –∫ –∫–æ—Ç–æ—Ä–æ–π –±—É–¥—É—Ç –ø—Ä–∏–∫—Ä–µ–ø–ª–µ–Ω—ã —Ñ–∏–∑–∏—á–µ—Å–∫–∏–µ –¥–≤–∏–∂–∫–∏.
        - WheelFrontLeft (Wheel)
        - WheelFrontRight (Wheel)
        - WheelBackLeft (Wheel)
        - WheelBackRight (Wheel)
    –¢–∞–∫ –∂–µ –ø—Ä–µ–¥–ø–æ–ª–∞–≥–∞–µ—Ç, —á—Ç–æ –Ω–∞ 'Body' —É–∂–µ –Ω–∞—Å—Ç—Ä–æ–µ–Ω BodyVelocity –∏–ª–∏ AlignPosition
    –¥–ª—è –ø–µ—Ä–µ–º–µ—â–µ–Ω–∏—è.

    –ö–ê–ö –ò–°–ü–û–õ–¨–ó–û–í–ê–¢–¨:
    1. –°–æ–∑–¥–∞–π—Ç–µ –º–æ–¥–µ–ª—å –º–∞—à–∏–Ω—ã –≤ Roblox Studio –∏–ª–∏ –Ω–∞–π–¥–∏—Ç–µ –≥–æ—Ç–æ–≤—É—é.
    2. –£–±–µ–¥–∏—Ç–µ—Å—å, —á—Ç–æ –º–æ–¥–µ–ª—å –∏–º–µ–µ—Ç —É–∫–∞–∑–∞–Ω–Ω—É—é —Å—Ç—Ä—É–∫—Ç—É—Ä—É.
    3. –ù–∞—Å—Ç—Ä–æ–π—Ç–µ BodyVelocity –Ω–∞ –æ—Å–Ω–æ–≤–Ω–æ–π —á–∞—Å—Ç–∏ (Body) –≤–∞—à–µ–π –º–∞—à–∏–Ω—ã.
       - MaxForce, P, D –±—É–¥—É—Ç –æ–ø—Ä–µ–¥–µ–ª—è—Ç—å —Å–∫–æ—Ä–æ—Å—Ç—å –∏ –æ—Ç–∑—ã–≤—á–∏–≤–æ—Å—Ç—å.
    4. "–ò–Ω–∂–µ–∫—Ç–∏—Ä—É–π—Ç–µ" —ç—Ç–æ—Ç —Å–∫—Ä–∏–ø—Ç –≤ –∫–ª–∏–µ–Ω—Ç –∏–≥—Ä—ã.
]]

-- –ü–∞—Ä–∞–º–µ—Ç—Ä—ã –º–∞—à–∏–Ω—ã
local CAR_MODEL_NAME = "MyAwesomeCar" -- –£–∫–∞–∂–∏—Ç–µ –∏–º—è –≤–∞—à–µ–π –º–æ–¥–µ–ª–∏ –º–∞—à–∏–Ω—ã
local STEERING_FORCE = 5000 -- –°–∏–ª–∞ –ø–æ–≤–æ—Ä–æ—Ç–∞
local ENGINE_FORCE = 20000 -- –°–∏–ª–∞ —É—Å–∫–æ—Ä–µ–Ω–∏—è
local BRAKE_FORCE = 30000  -- –°–∏–ª–∞ —Ç–æ—Ä–º–æ–∂–µ–Ω–∏—è
local MAX_SPEED = 100      -- –û–≥—Ä–∞–Ω–∏—á–µ–Ω–∏–µ –º–∞–∫—Å–∏–º–∞–ª—å–Ω–æ–π —Å–∫–æ—Ä–æ—Å—Ç–∏ (–¥–ª—è BodyVelocity)

-- –ù–∞—Å—Ç—Ä–æ–π–∫–∏ —É–ø—Ä–∞–≤–ª–µ–Ω–∏—è
local FORWARD_KEY = Enum.KeyCode.W
local BACKWARD_KEY = Enum.KeyCode.S
local LEFT_KEY = Enum.KeyCode.A
local RIGHT_KEY = Enum.KeyCode.D
local BRAKE_KEY = Enum.KeyCode.Space

-- –ì–ª–æ–±–∞–ª—å–Ω—ã–µ –ø–µ—Ä–µ–º–µ–Ω–Ω—ã–µ
local carModel = nil
local player = game.Players.LocalPlayer
local userInputService = game:GetService("UserInputService")
local runService = game:GetService("RunService")

local currentThrottle = 0
local currentSteering = 0
local isBraking = false

-- –ü–æ–∏—Å–∫ –º–æ–¥–µ–ª–∏ –º–∞—à–∏–Ω—ã
local function findCarModel()
    for _, model in pairs(workspace:GetChildren()) do
        if model:IsA("Model") and model.Name == CAR_MODEL_NAME then
            -- –ü—Ä–æ–≤–µ—Ä—è–µ–º, –µ—Å—Ç—å –ª–∏ –Ω–µ–æ–±—Ö–æ–¥–∏–º—ã–µ —á–∞—Å—Ç–∏
            if model:FindFirstChild("Body") and
               model:FindFirstChild("WheelFrontLeft") and
               model:FindFirstChild("WheelFrontRight") and
               model:FindFirstChild("WheelBackLeft") and
               model:FindFirstChild("WheelBackRight") then
                return model
            end
        end
    end
    return nil
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –æ–±—Ä–∞–±–æ—Ç–∫–∏ –≤–≤–æ–¥–∞ –∏–≥—Ä–æ–∫–∞
local function handleInput(input, gameProcessedEvent)
    if gameProcessedEvent then return end

    local key = input.KeyCode

    if input.UserInputType == Enum.UserInputType.Keyboard then
        if key == FORWARD_KEY then
            currentThrottle = 1
        elseif key == BACKWARD_KEY then
            currentThrottle = -1
        elseif key == LEFT_KEY then
            currentSteering = -1
        elseif key == RIGHT_KEY then
            currentSteering = 1
        elseif key == BRAKE_KEY then
            isBraking = true
        end
    end
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–±—Ä–æ—Å–∞ —É–ø—Ä–∞–≤–ª–µ–Ω–∏—è
local function resetControls()
    currentThrottle = 0
    currentSteering = 0
    isBraking = false
end

-- –û–±—Ä–∞–±–æ—Ç–∫–∞ –æ—Ç–ø—É—Å–∫–∞–Ω–∏—è –∫–ª–∞–≤–∏—à
local function handleInputEnded(input, gameProcessedEvent)
    if gameProcessedEvent then return end

    local key = input.KeyCode

    if input.UserInputType == Enum.UserInputType.Keyboard then
        if key == FORWARD_KEY and currentThrottle == 1 then
            currentThrottle = 0
        elseif key == BACKWARD_KEY and currentThrottle == -1 then
            currentThrottle = 0
        elseif key == LEFT_KEY and currentSteering == -1 then
            currentSteering = 0
        elseif key == RIGHT_KEY and currentSteering == 1 then
            currentSteering = 0
        elseif key == BRAKE_KEY then
            isBraking = false
        end
    end
end

-- –û—Å–Ω–æ–≤–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è —É–ø—Ä–∞–≤–ª–µ–Ω–∏—è –º–∞—à–∏–Ω–æ–π
local function updateCar()
    if not carModel then return end

    local body = carModel:FindFirstChild("Body")
    if not body then return end

    local bodyVelocity = body:FindFirstChildOfClass("BodyVelocity")
    if not bodyVelocity then
        -- –ï—Å–ª–∏ BodyVelocity –Ω–µ –Ω–∞–π–¥–µ–Ω, –º–æ–∂–Ω–æ –¥–æ–±–∞–≤–∏—Ç—å –µ–≥–æ –∏–ª–∏ –≤—ã–≤–µ—Å—Ç–∏ —Å–æ–æ–±—â–µ–Ω–∏–µ
        warn("BodyVelocity –Ω–µ –Ω–∞–π–¥–µ–Ω –Ω–∞ —á–∞—Å—Ç–∏ 'Body' –º–∞—à–∏–Ω—ã: " .. carModel.Name)
        return
    end

    local direction = body.CFrame.LookVector * currentThrottle
    local moveForce = direction * ENGINE_FORCE

    -- –£–ø—Ä–∞–≤–ª–µ–Ω–∏–µ —É—Å–∫–æ—Ä–µ–Ω–∏–µ–º –∏ —Ç–æ—Ä–º–æ–∂–µ–Ω–∏–µ–º
    if isBraking then
        bodyVelocity.Velocity = Vector3.new(0, 0, 0) -- –ü–æ–ª–Ω–∞—è –æ—Å—Ç–∞–Ω–æ–≤–∫–∞
        bodyVelocity.MaxForce = Vector3.new(math.huge, 0, math.huge) * BRAKE_FORCE -- –ü—Ä–∏–º–µ–Ω—è–µ–º —Å–∏–ª—å–Ω–æ–µ —Ç–æ—Ä–º–æ–∂–µ–Ω–∏–µ
    else
        bodyVelocity.MaxForce = Vector3.new(math.huge, 0, math.huge) * ENGINE_FORCE -- –ü—Ä–∏–º–µ–Ω—è–µ–º —Å–∏–ª—É –¥–≤–∏–∂–µ–Ω–∏—è
        bodyVelocity.Velocity = direction * MAX_SPEED -- –û–≥—Ä–∞–Ω–∏—á–∏–≤–∞–µ–º —Å–∫–æ—Ä–æ—Å—Ç—å
    end

    -- –£–ø—Ä–∞–≤–ª–µ–Ω–∏–µ –ø–æ–≤–æ—Ä–æ—Ç–æ–º (–ø—Ä–∏–º–µ—Ä–Ω–æ, –º–æ–∂–µ—Ç –ø–æ—Ç—Ä–µ–±–æ–≤–∞—Ç—å—Å—è –Ω–∞—Å—Ç—Ä–æ–π–∫–∞)
    -- –î–ª—è –ø–æ–≤–æ—Ä–æ—Ç–∞ –Ω—É–∂–Ω–æ –∏–∑–º–µ–Ω—è—Ç—å CFrame –∏–ª–∏ –ø—Ä–∏–º–µ–Ω—è—Ç—å —Å–∏–ª—ã –∫ –∫–æ–ª–µ—Å–∞–º
    -- –≠—Ç–æ—Ç –ø—Ä–∏–º–µ—Ä —É–ø—Ä–æ—â–µ–Ω –∏ –ø—Ä–µ–¥–ø–æ–ª–∞–≥–∞–µ—Ç, —á—Ç–æ BodyVelocity –º–æ–∂–µ—Ç –±—ã—Ç—å
    -- –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∞, –Ω–æ —Ä–µ–∞–ª—å–Ω—ã–π –ø–æ–≤–æ—Ä–æ—Ç —Ç—Ä–µ–±—É–µ—Ç –≤—Ä–∞—â–µ–Ω–∏—è –∫–æ–ª–µ—Å.

    -- –ü—Ä–∏–º–µ—Ä –¥–ª—è –ø–æ–≤–æ—Ä–æ—Ç–∞:
    local steeringAxis = carModel.CFrame.UpVector -- –û—Å—å –ø–æ–≤–æ—Ä–æ—Ç–∞ (–ø—Ä–µ–¥–ø–æ–ª–∞–≥–∞–µ–º, —á—Ç–æ Y - —ç—Ç–æ –≤–≤–µ—Ä—Ö)

    -- –ü–æ–≤–æ—Ä–æ—Ç –ø–µ—Ä–µ–¥–Ω–∏—Ö –∫–æ–ª–µ—Å (–¥–ª—è –±–æ–ª–µ–µ —Ä–µ–∞–ª–∏—Å—Ç–∏—á–Ω–æ–≥–æ —É–ø—Ä–∞–≤–ª–µ–Ω–∏—è)
    -- –≠—Ç–æ —Å–ª–æ–∂–Ω–∞—è —á–∞—Å—Ç—å, —Ç—Ä–µ–±—É—é—â–∞—è –æ–±—Ä–∞—â–µ–Ω–∏—è –∫ –º–µ—Ö–∞–Ω–∏–∑–º–∞–º –≤—Ä–∞—â–µ–Ω–∏—è –∫–æ–ª–µ—Å.
    -- –í —ç—Ç–æ–º –ø—Ä–æ—Å—Ç–æ–º –ø—Ä–∏–º–µ—Ä–µ –º—ã –º–æ–∂–µ–º –ø–æ–ø—ã—Ç–∞—Ç—å—Å—è –Ω–µ–º–Ω–æ–≥–æ –ø–æ–≤–µ—Ä–Ω—É—Ç—å –≤–µ—Å—å –∫–æ—Ä–ø—É—Å.

    if currentSteering ~= 0 then
        -- –ü–æ–≤–æ—Ä–æ—Ç –∫—É–∑–æ–≤–∞ (–æ—á–µ–Ω—å —É–ø—Ä–æ—â–µ–Ω–Ω–æ)
        local turnAmount = STEERING_FORCE * currentSteering
        carModel:SetPivot(carModel:GetPivot() * CFrame.Angles(0, math.rad(turnAmount * runService.Heartbeat:Wait()), 0))
    end
end

-- –û—Å–Ω–æ–≤–Ω–æ–π —Ü–∏–∫–ª —Å–∫—Ä–∏–ø—Ç–∞
local function gameLoop()
    if not carModel then
        carModel = findCarModel()
        if carModel then
            print("–ú–∞—à–∏–Ω–∞ '" .. CAR_MODEL_NAME .. "' –Ω–∞–π–¥–µ–Ω–∞!")
            -- –ü–æ–¥–ø–∏—Å—ã–≤–∞–µ–º—Å—è –Ω–∞ –≤–≤–æ–¥ –∏–≥—Ä–æ–∫–∞
            userInputService.InputBegan:Connect(handleInput)
            userInputService.InputEnded:Connect(handleInputEnded)
        end
    end

    if carModel then
        updateCar()
    end
end

-- –ü–æ–¥–ø–∏—Å–∫–∞ –Ω–∞ —Å–æ–±—ã—Ç–∏–µ Heartbeat –¥–ª—è –ø–æ—Å—Ç–æ—è–Ω–Ω–æ–≥–æ –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è
runService.Heartbeat:Connect(gameLoop)

print("–°–∫—Ä–∏–ø—Ç —É–ø—Ä–∞–≤–ª—è–µ–º–æ–π –º–∞—à–∏–Ω—ã –∑–∞–ø—É—â–µ–Ω. –ò—â–µ—Ç –º–æ–¥–µ–ª—å '" .. CAR_MODEL_NAME .. "'...")
end)


Section:NewToggle("ToggleText", "ToggleInfo", function(state)
    if state then
        print("Toggle On")
    else
        print("Toggle Off")
    end
end)
