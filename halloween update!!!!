-- ÐŸÐ°Ñ€Ð°Ð¼ÐµÑ‚Ñ€Ñ‹ Ð¼Ð°ÑˆÐ¸Ð½Ñ‹
local CAR_MODEL_NAME = "MyAwesomeCar" -- Ð£ÐºÐ°Ð¶Ð¸Ñ‚Ðµ Ð¸Ð¼Ñ Ð²Ð°ÑˆÐµÐ¹ Ð¼Ð¾Ð´ÐµÐ»Ð¸ Ð¼Ð°ÑˆÐ¸Ð½Ñ‹
local STEERING_FORCE = 5000 -- Ð¡Ð¸Ð»Ð° Ð¿Ð¾Ð²Ð¾Ñ€Ð¾Ñ‚Ð°
local ENGINE_FORCE = 20000 -- Ð¡Ð¸Ð»Ð° ÑƒÑÐºÐ¾Ñ€ÐµÐ½Ð¸Ñ
local BRAKE_FORCE = 30000  -- Ð¡Ð¸Ð»Ð° Ñ‚Ð¾Ñ€Ð¼Ð¾Ð¶ÐµÐ½Ð¸Ñ
local MAX_SPEED = 100      -- ÐžÐ³Ñ€Ð°Ð½Ð¸Ñ‡ÐµÐ½Ð¸Ðµ Ð¼Ð°ÐºÑÐ¸Ð¼Ð°Ð»ÑŒÐ½Ð¾Ð¹ ÑÐºÐ¾Ñ€Ð¾ÑÑ‚Ð¸ (Ð´Ð»Ñ BodyVelocity)

-- ÐÐ°ÑÑ‚Ñ€Ð¾Ð¹ÐºÐ¸ ÑƒÐ¿Ñ€Ð°Ð²Ð»ÐµÐ½Ð¸Ñ
local FORWARD_KEY = Enum.KeyCode.W
local BACKWARD_KEY = Enum.KeyCode.S
local LEFT_KEY = Enum.KeyCode.A
local RIGHT_KEY = Enum.KeyCode.D
local BRAKE_KEY = Enum.KeyCode.Space

-- Ð“Ð»Ð¾Ð±Ð°Ð»ÑŒÐ½Ñ‹Ðµ Ð¿ÐµÑ€ÐµÐ¼ÐµÐ½Ð½Ñ‹Ðµ
local carModel = nil
local player = game.Players.LocalPlayer
local userInputService = game:GetService("UserInputService")
local runService = game:GetService("RunService")

local currentThrottle = 0
local currentSteering = 0
local isBraking = false

-- ÐŸÐ¾Ð¸ÑÐº Ð¼Ð¾Ð´ÐµÐ»Ð¸ Ð¼Ð°ÑˆÐ¸Ð½Ñ‹
local function findCarModel()
    for _, model in pairs(workspace:GetChildren()) do
        if model:IsA("Model") and model.Name == CAR_MODEL_NAME then
            -- ÐŸÑ€Ð¾Ð²ÐµÑ€ÑÐµÐ¼, ÐµÑÑ‚ÑŒ Ð»Ð¸ Ð½ÐµÐ¾Ð±Ñ…Ð¾Ð´Ð¸Ð¼Ñ‹Ðµ Ñ‡Ð°ÑÑ‚Ð¸
            if model:FindFirstChild("Body") and
               model:FindFirstChild("WheelFrontLeft") and
               model:FindFirstChild("WheelFrontRight") and
               model:FindFirstChild("WheelBackLeft") and
               model:FindFirstChild("WheelBackRight") then
                return model
            end
        end
    end
    return nil
end

-- Ð¤ÑƒÐ½ÐºÑ†Ð¸Ñ Ð´Ð»Ñ Ð¾Ð±Ñ€Ð°Ð±Ð¾Ñ‚ÐºÐ¸ Ð²Ð²Ð¾Ð´Ð° Ð¸Ð³Ñ€Ð¾ÐºÐ°
local function handleInput(input, gameProcessedEvent)
    if gameProcessedEvent then return end

    local key = input.KeyCode

    if input.UserInputType == Enum.UserInputType.Keyboard then
        if key == FORWARD_KEY then
            currentThrottle = 1
        elseif key == BACKWARD_KEY then
            currentThrottle = -1
        elseif key == LEFT_KEY then
            currentSteering = -1
        elseif key == RIGHT_KEY then
            currentSteering = 1
        elseif key == BRAKE_KEY then
            isBraking = true
        end
    end
end

-- Ð¤ÑƒÐ½ÐºÑ†Ð¸Ñ Ð´Ð»Ñ ÑÐ±Ñ€Ð¾ÑÐ° ÑƒÐ¿Ñ€Ð°Ð²Ð»ÐµÐ½Ð¸Ñ
local function resetControls()
    currentThrottle = 0
    currentSteering = 0
    isBraking = false
end

-- ÐžÐ±Ñ€Ð°Ð±Ð¾Ñ‚ÐºÐ° Ð¾Ñ‚Ð¿ÑƒÑÐºÐ°Ð½Ð¸Ñ ÐºÐ»Ð°Ð²Ð¸Ñˆ
local function handleInputEnded(input, gameProcessedEvent)
    if gameProcessedEvent then return end

    local key = input.KeyCode

    if input.UserInputType == Enum.UserInputType.Keyboard then
        if key == FORWARD_KEY and currentThrottle == 1 then
            currentThrottle = 0
        elseif key == BACKWARD_KEY and currentThrottle == -1 then
            currentThrottle = 0
        elseif key == LEFT_KEY and currentSteering == -1 then
            currentSteering = 0
        elseif key == RIGHT_KEY and currentSteering == 1 then
            currentSteering = 0
        elseif key == BRAKE_KEY then
            isBraking = false
        end
    end
end

-- ÐžÑÐ½Ð¾Ð²Ð½Ð°Ñ Ñ„ÑƒÐ½ÐºÑ†Ð¸Ñ ÑƒÐ¿Ñ€Ð°Ð²Ð»ÐµÐ½Ð¸Ñ Ð¼Ð°ÑˆÐ¸Ð½Ð¾Ð¹
local function updateCar()
    if not carModel then return end

    local body = carModel:FindFirstChild("Body")
    if not body then return end

    local bodyVelocity = body:FindFirstChildOfClass("BodyVelocity")
    if not bodyVelocity then
        -- Ð•ÑÐ»Ð¸ BodyVelocity Ð½Ðµ Ð½Ð°Ð¹Ð´ÐµÐ½, Ð¼Ð¾Ð¶Ð½Ð¾ Ð´Ð¾Ð±Ð°Ð²Ð¸Ñ‚ÑŒ ÐµÐ³Ð¾ Ð¸Ð»Ð¸ Ð²Ñ‹Ð²ÐµÑÑ‚Ð¸ ÑÐ¾Ð¾Ð±Ñ‰ÐµÐ½Ð¸Ðµ
        warn("BodyVelocity Ð½Ðµ Ð½Ð°Ð¹Ð´ÐµÐ½ Ð½Ð° Ñ‡Ð°ÑÑ‚Ð¸ 'Body' Ð¼Ð°ÑˆÐ¸Ð½Ñ‹: " .. carModel.Name)
        return
    end

    local direction = body.CFrame.LookVector * currentThrottle
    local moveForce = direction * ENGINE_FORCE

    -- Ð£Ð¿Ñ€Ð°Ð²Ð»ÐµÐ½Ð¸Ðµ ÑƒÑÐºÐ¾Ñ€ÐµÐ½Ð¸ÐµÐ¼ Ð¸ Ñ‚Ð¾Ñ€Ð¼Ð¾Ð¶ÐµÐ½Ð¸ÐµÐ¼
    if isBraking then
        bodyVelocity.Velocity = Vector3.new(0, 0, 0) -- ÐŸÐ¾Ð»Ð½Ð°Ñ Ð¾ÑÑ‚Ð°Ð½Ð¾Ð²ÐºÐ°
        bodyVelocity.MaxForce = Vector3.new(math.huge, 0, math.huge) * BRAKE_FORCE -- ÐŸÑ€Ð¸Ð¼ÐµÐ½ÑÐµÐ¼ ÑÐ¸Ð»ÑŒÐ½Ð¾Ðµ Ñ‚Ð¾Ñ€Ð¼Ð¾Ð¶ÐµÐ½Ð¸Ðµ
    else
        bodyVelocity.MaxForce = Vector3.new(math.huge, 0, math.huge) * ENGINE_FORCE -- ÐŸÑ€Ð¸Ð¼ÐµÐ½ÑÐµÐ¼ ÑÐ¸Ð»Ñƒ Ð´Ð²Ð¸Ð¶ÐµÐ½Ð¸Ñ
        bodyVelocity.Velocity = direction * MAX_SPEED -- ÐžÐ³Ñ€Ð°Ð½Ð¸Ñ‡Ð¸Ð²Ð°ÐµÐ¼ ÑÐºÐ¾Ñ€Ð¾ÑÑ‚ÑŒ
    end

    -- Ð£Ð¿Ñ€Ð°Ð²Ð»ÐµÐ½Ð¸Ðµ Ð¿Ð¾Ð²Ð¾Ñ€Ð¾Ñ‚Ð¾Ð¼ (Ð¿Ñ€Ð¸Ð¼ÐµÑ€Ð½Ð¾, Ð¼Ð¾Ð¶ÐµÑ‚ Ð¿Ð¾Ñ‚Ñ€ÐµÐ±Ð¾Ð²Ð°Ñ‚ÑŒÑÑ Ð½Ð°ÑÑ‚Ñ€Ð¾Ð¹ÐºÐ°)
    -- Ð”Ð»Ñ Ð¿Ð¾Ð²Ð¾Ñ€Ð¾Ñ‚Ð° Ð½ÑƒÐ¶Ð½Ð¾ Ð¸Ð·Ð¼ÐµÐ½ÑÑ‚ÑŒ CFrame Ð¸Ð»Ð¸ Ð¿Ñ€Ð¸Ð¼ÐµÐ½ÑÑ‚ÑŒ ÑÐ¸Ð»Ñ‹ Ðº ÐºÐ¾Ð»ÐµÑÐ°Ð¼
    -- Ð­Ñ‚Ð¾Ñ‚ Ð¿Ñ€Ð¸Ð¼ÐµÑ€ ÑƒÐ¿Ñ€Ð¾Ñ‰ÐµÐ½ Ð¸ Ð¿Ñ€ÐµÐ´Ð¿Ð¾Ð»Ð°Ð³Ð°ÐµÑ‚, Ñ‡Ñ‚Ð¾ BodyVelocity Ð¼Ð¾Ð¶ÐµÑ‚ Ð±Ñ‹Ñ‚ÑŒ
    -- Ð½Ð°Ð¿Ñ€Ð°Ð²Ð»ÐµÐ½Ð°, Ð½Ð¾ Ñ€ÐµÐ°Ð»ÑŒÐ½Ñ‹Ð¹ Ð¿Ð¾Ð²Ð¾Ñ€Ð¾Ñ‚ Ñ‚Ñ€ÐµÐ±ÑƒÐµÑ‚ Ð²Ñ€Ð°Ñ‰ÐµÐ½Ð¸Ñ ÐºÐ¾Ð»ÐµÑ.

    -- ÐŸÑ€Ð¸Ð¼ÐµÑ€ Ð´Ð»Ñ Ð¿Ð¾Ð²Ð¾Ñ€Ð¾Ñ‚Ð°:
    local steeringAxis = carModel.CFrame.UpVector -- ÐžÑÑŒ Ð¿Ð¾Ð²Ð¾Ñ€Ð¾Ñ‚Ð° (Ð¿Ñ€ÐµÐ´Ð¿Ð¾Ð»Ð°Ð³Ð°ÐµÐ¼, Ñ‡Ñ‚Ð¾ Y - ÑÑ‚Ð¾ Ð²Ð²ÐµÑ€Ñ…)

    -- ÐŸÐ¾Ð²Ð¾Ñ€Ð¾Ñ‚ Ð¿ÐµÑ€ÐµÐ´Ð½Ð¸Ñ… ÐºÐ¾Ð»ÐµÑ (Ð´Ð»Ñ Ð±Ð¾Ð»ÐµÐµ Ñ€ÐµÐ°Ð»Ð¸ÑÑ‚Ð¸Ñ‡Ð½Ð¾Ð³Ð¾ ÑƒÐ¿Ñ€Ð°Ð²Ð»ÐµÐ½Ð¸Ñ)
    -- Ð­Ñ‚Ð¾ ÑÐ»Ð¾Ð¶Ð½Ð°Ñ Ñ‡Ð°ÑÑ‚ÑŒ, Ñ‚Ñ€ÐµÐ±ÑƒÑŽÑ‰Ð°Ñ Ð¾Ð±Ñ€Ð°Ñ‰ÐµÐ½Ð¸Ñ Ðº Ð¼ÐµÑ…Ð°Ð½Ð¸Ð·Ð¼Ð°Ð¼ Ð²Ñ€Ð°Ñ‰ÐµÐ½Ð¸Ñ ÐºÐ¾Ð»ÐµÑ.
    -- Ð’ ÑÑ‚Ð¾Ð¼ Ð¿Ñ€Ð¾ÑÑ‚Ð¾Ð¼ Ð¿Ñ€Ð¸Ð¼ÐµÑ€Ðµ Ð¼Ñ‹ Ð¼Ð¾Ð¶ÐµÐ¼ Ð¿Ð¾Ð¿Ñ‹Ñ‚Ð°Ñ‚ÑŒÑÑ Ð½ÐµÐ¼Ð½Ð¾Ð³Ð¾ Ð¿Ð¾Ð²ÐµÑ€Ð½ÑƒÑ‚ÑŒ Ð²ÐµÑÑŒ ÐºÐ¾Ñ€Ð¿ÑƒÑ.

    if currentSteering ~= 0 then
        -- ÐŸÐ¾Ð²Ð¾Ñ€Ð¾Ñ‚ ÐºÑƒÐ·Ð¾Ð²Ð° (Ð¾Ñ‡ÐµÐ½ÑŒ ÑƒÐ¿Ñ€Ð¾Ñ‰ÐµÐ½Ð½Ð¾)
        local turnAmount = STEERING_FORCE * currentSteering
        carModel:SetPivot(carModel:GetPivot() * CFrame.Angles(0, math.rad(turnAmount * runService.Heartbeat:Wait()), 0))
    end
end

-- ÐžÑÐ½Ð¾Ð²Ð½Ð¾Ð¹ Ñ†Ð¸ÐºÐ» ÑÐºÑ€Ð¸Ð¿Ñ‚Ð°
local function gameLoop()
    if not carModel then
        carModel = findCarModel()
        if carModel then
            print("ÐœÐ°ÑˆÐ¸Ð½Ð° '" .. CAR_MODEL_NAME .. "' Ð½Ð°Ð¹Ð´ÐµÐ½Ð°!")
            -- ÐŸÐ¾Ð´Ð¿Ð¸ÑÑ‹Ð²Ð°ÐµÐ¼ÑÑ Ð½Ð° Ð²Ð²Ð¾Ð´ Ð¸Ð³Ñ€Ð¾ÐºÐ°
            userInputService.InputBegan:Connect(handleInput)
            userInputService.InputEnded:Connect(handleInputEnded)
        end
    end

    if carModel then
        updateCar()
    end
end

-- ÐŸÐ¾Ð´Ð¿Ð¸ÑÐºÐ° Ð½Ð° ÑÐ¾Ð±Ñ‹Ñ‚Ð¸Ðµ Heartbeat Ð´Ð»Ñ Ð¿Ð¾ÑÑ‚Ð¾ÑÐ½Ð½Ð¾Ð³Ð¾ Ð¾Ð±Ð½Ð¾Ð²Ð»ÐµÐ½Ð¸Ñ
runService.Heartbeat:Connect(gameLoop)

print("Ð¡ÐºÑ€Ð¸Ð¿Ñ‚ ÑƒÐ¿Ñ€Ð°Ð²Ð»ÑÐµÐ¼Ð¾Ð¹ Ð¼Ð°ÑˆÐ¸Ð½Ñ‹ Ð·Ð°Ð¿ÑƒÑ‰ÐµÐ½. Ð˜Ñ‰ÐµÑ‚ Ð¼Ð¾Ð´ÐµÐ»ÑŒ '" .. CAR_MODEL_NAME .. "'...")
end)


